{
  "name": "node-red-contrib-tensorflow-js",
  "version": "0.0.8",
  "description": "A node for node-red that uses tensorflow.js to detect custom objects",
  "license": "MIT",
  "scripts": {
    "start": "nodemon  --ext html,htm,js,json --exec \"node-red\""
  },
  "keywords": [
    "node-red"
  ],
  "node-red": {
    "nodes": {
      "object-detection": "object-detection.js"
    }
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^1.2.5",
    "canvas": "^2.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
